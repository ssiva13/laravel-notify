{
    "name": "ssiva/laravel-notify",
    "description": "Laravel Notifications library",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "ext-simplexml": "*",
        "orchestra/testbench": "^8.3",
        "darkaonline/l5-swagger": "^8.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ssiva\\LaravelNotify\\": "src/"
        }
    },
    "autoloaddev": {
        "psr-4": {
            "Ssiva\\LaravelNotify\\Tests": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Simon Siva",
            "email": "simonsiva13@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "\\Ssiva\\LaravelNotify\\LaravelNotifyServiceProvider"
            ]
        }
    }
}
